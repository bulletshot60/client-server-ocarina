package FullSystem
public
	with Client, Server;
	
	system full_system
	end full_system;
	
	system implementation full_system.impl
		subcomponents
			my_client: system Client::my_client.impl;
			my_server: system Server::my_server.impl;
		connections
			client_to_server: port my_client.out_data -> my_server.in_data;
			server_to_client: port my_server.out_data -> my_client.in_data;
	end full_system.impl;
	
end FullSystem;
